<!--index.wxml-->
 
  
<view class="container">
    <view class='chart-title'>
        <view class="chart-with-img" style="flex-direction:row;" wx:for="{{charts}}" wx:for-item="chart" bindtap="open"         data-chart="{{chart}}" wx:key="{{chart.id}}">
            {{chart.name}}
        </view>
    </view>

  <ec-canvas id="mychart-dom-line" canvas-id="mychart-line" ec="{{ ec }}"></ec-canvas>

   
</view>
